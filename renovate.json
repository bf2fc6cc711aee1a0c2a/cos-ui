{
  "extends": [ "github>redhat-developer/app-services-renovate-config" ],
  "ignoreDeps": ["parcel"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^uniforms"],
      "groupName": "uniforms"
    },
    {
      "matchPackagePatterns": ["^@?xstate"],
      "groupName": "xstate"
    }
  ]
}
