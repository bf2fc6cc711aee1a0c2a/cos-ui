  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "lerna": "lerna",
    "start": "lerna run start --stream --parallel --scope @cos-ui/*",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "lerna run build",
    "prepublish": "yarn run api:generate && lerna run build",
    "start:app:example": "yarn run build && yarn start:app:example:run",
    "start:app:example:run": "yarn --cwd example && yarn --cwd example start",
    "start:app:with-sample-configurators": "lerna run start --stream --parallel --scope @cos-ui-apps/* --ignore @cos-ui-apps/cli",
    "start:federate": "lerna run start:federate --stream --parallel --scope @cos-ui-apps/cos-ui",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:canary": "changeset version --snapshot canary",
    "release:canary": "changeset publish --tag canary",
    "api:generate": "yarn run api:generate:merge && yarn run api:generate:build",
    "api:generate:merge": "openapi-merge-cli && openapi-generator-cli generate -i mas-mock/merged-openapi.yaml -g typescript-axios -o packages/api --additional-properties=ngVersion=6.1.7,npmName=@cos-ui/api,supportsES6=true,withInterfaces=true --skip-validate-spec && rimraf packages/api/dist",
    "api:generate:build": "lerna run build --scope @cos-ui/api"
  },
  "eslint": {
    "extends": [
      "plugin:jest-dom/recommended",
      "plugin:react-hooks/recommended",
      "plugin:testing-library/recommended",
      "plugin:testing-library/react"
    ],
    "plugins": [
      "react-hooks",
      "jest-dom",
      "testing-library"
    ],
    "overrides": [
      {
        "env": {
          "node": true,
          "commonjs": true,
          "browser": true,
          "jest": true
        }
      }
    ]
  },
  "resolutions": {
    "@types/webpack": "^5.28.0",
    "**/typescript": "^4.3.2"
  }
}
