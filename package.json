  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "lerna": "lerna",
    "start": "lerna run start --stream --parallel --scope @cos-ui/*",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "yarn run clear && lerna run build",
    "prepublish": "yarn run api:generate && lerna run build",
    "start:app:e2e": "yarn --cwd example && yarn --cwd example e2e",
    "start:app:example": "yarn run build --scope '@cos-ui/*' && yarn start:app:example:run",
    "start:app:example:run": "yarn --cwd example && yarn --cwd example start",
    "start:app:with-sample-configurators": "lerna run start --stream --parallel --scope @cos-ui-apps/* --ignore @cos-ui-apps/cli",
    "start:federate": "lerna run start:federate --stream --parallel --scope @cos-ui-apps/cos-ui",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:canary": "changeset version --snapshot canary",
    "release:canary": "changeset publish --tag canary",
    "api:generate": "yarn run api:generate:merge && yarn run api:generate:build",
    "api:generate:merge": "openapi-merge-cli && openapi-generator-cli generate -i ./openapi/merged-openapi.yaml -g typescript-axios -o packages/api --additional-properties=ngVersion=6.1.7,npmName=@cos-ui/api,supportsES6=true,withInterfaces=true --skip-validate-spec && rimraf packages/api/dist",
    "api:generate:build": "lerna run build --scope @cos-ui/api",
    "i18n": "i18next 'packages/**/*.{ts,tsx}' [-oc] -c './i18next-parser.config.js' -o 'apps/cos-ui/src/locales/$LOCALE/$NAMESPACE.json'",
    "clear": "find packages -name dist -exec rm -rf {} + && find packages -name .cache -exec rm -rf {} +"
  },
  "eslint": {
    "extends": [
      "plugin:jest-dom/recommended",
      "plugin:react-hooks/recommended",
      "plugin:testing-library/recommended",
      "plugin:testing-library/react"
    ],
    "plugins": [
      "react-hooks",
      "jest-dom",
      "testing-library"
    ],
    "overrides": [
      {
        "env": {
          "node": true,
          "commonjs": true,
          "browser": true,
          "jest": true
        }
      }
    ]
  },
  "resolutions": {
    "@types/webpack": "5.28.0",
    "**/@typescript-eslint/eslint-plugin": "4.28.5",
    "**/@typescript-eslint/parser": "4.28.5",
